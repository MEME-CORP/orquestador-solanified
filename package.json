    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "@supabase/supabase-js": "^2.38.4",
    "pg": "^8.11.3",
    "axios": "^1.6.2",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0"
  },
  "keywords": ["crypto", "wallet", "orchestrator", "solana"],
  "author": "Your Name",
  "license": "MIT"
}
